{
  "name": "cardle",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.20.0",
    "connect-mongo": "^4.6.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mongoose": "^6.5.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "proxy": "http://localhost:5125",
  "scripts": {
